{
    "version": 1,
    "paths": {
        "plugins": "/tmp/cocaine/lib/cocaine",
        "ipc": "/tmp/cocaine/ipc",
        "spool": "/tmp/cocaine/spool"
    },
    "storages": {
        "core": {
            "type": "elliptics",
            "args": {
                "nodes": {
                    "localhost": 1025
                },
                "groups": [1, 2, 3],
                "verbosity": 10
            }
        },
        "core:cache": {
            "type": "files",
            "args": {
                "path": "/tmp/cocaine/cache"
            }
        }
    }
}
